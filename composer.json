{
    "name": "php-web-component/assets-manager",
    "description": "PHP Web Component Assets Manager",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Bagus A Priambada",
            "email": "iam.adty@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "PWC\\CLI\\": "cli/",
            "PWC\\AssetsManager\\": "src/"
        }
    },
    "require": {
        "php-web-component/cli": "dev-develop",
        "php-web-component/asset": "dev-develop",
        "jc21/clitable": "^1.2"
    },
    "scripts": {
        "pwc": "vendor/bin/pwc"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "http://localhost/satis"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "config": {
        "secure-http": false
    }
}
